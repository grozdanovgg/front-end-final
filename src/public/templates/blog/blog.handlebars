<h1>All Categories</h1>


{{#each categories}}
<a href="#!blog/{{this.href}}" class="d-inline-block">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">{{this.title}}</h4>
            <h6 class="card-subtitle mb-2 text-muted">{{this.description}}</h6>
        </div>
    </div>
</a>
{{/each}}